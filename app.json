{
	"expo": {
		"name": "proyectoFinalTortosa",
		"slug": "lumaveggieapp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/luma.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/burger.png",
			"resizeMode": "contain",
			"backgroundColor": "#F3D15F"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "com.lumaVeggie.app",
			"supportsTablet": true
		},
		"android": {
			"package": "com.lumaVeggie.app",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/burger.png",
				"backgroundColor": "#F3D15F"
			},
			"permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
		},
		"web": {
			"favicon": "./assets/lumafav.png"
		}
	}
}
